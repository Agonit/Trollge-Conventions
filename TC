--Script by KidAmongUs, DO NOT CHANGE ANYTHING IF U WANT IT TO WORK LOL

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("KidAmongUs", "DarkTheme")
if game.PlaceId == 9216815133 then
	-- Main
	local Main = Window:NewTab("Main")
	local MainSection = Main:NewSection("Main")


	MainSection:NewToggle("Active auto PickUp chests", "PickUp chests for you", function(state)
		if state then
			
				getgenv().hm = true --Change to false to turn off sped
				spawn(function()
					while hm do wait()
						pcall(function()
							for i,v in pairs(game:GetService("Workspace").chests:GetDescendants()) do
								if v:IsA("Part") then
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
									fireproximityprompt(v.ProximityPrompt)
								end
							end
						end)
					end
				end)
			else
				getgenv().hm = false --Change to false to turn off sped
				spawn(function()
					while hm do wait()
						pcall(function()
							for i,v in pairs(game:GetService("Workspace").chests:GetDescendants()) do
								if v:IsA("Part") then
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
									fireproximityprompt(v.ProximityPrompt)
								end
							end
						end)
					end
				end)

			end
		end)

MainSection:NewButton("Infinite Yield - Open Chests / Click here ->", "Type - UseTools - to open all chests in your inventory", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)

-- Player
local Player = Window:NewTab("Player")
local PlayerSection = Player:NewSection("Player")

PlayerSection:NewSlider("WalkSpeed", "Speed!", 500, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

PlayerSection:NewSlider("JumpPower", "Jump High!", 500, 50, function(s) -- 500 (MaxValue) | 0 (MinValue)
	game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
end
